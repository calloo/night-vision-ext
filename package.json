{
  "name": "night-vision-ext",
  "version": "0.4.5",
  "description": "Highly customizable charting library, created for professional traders",
  "homepage": "https://github.com/calloo/night-vision-ext",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && node ./types/build-script.cjs",
    "build-cdn": "vite build --config vite.cdn.config.js",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.svelte --fix",
    "format": "prettier --write 'src/**/*.{js,svelte}'"
  },
  "main": "./dist/night-vision-ext.js",
  "module": "./dist/night-vision-ext.js",
  "exports": {
    ".": {
      "import": "./dist/night-vision-ext.js",
      "require": "./dist/night-vision-ext.umd.cjs"
    }
  },
  "author": "ChartMaster",
  "license": "ISC",
  "dependencies": {
    "@freecodecamp/strip-comments": "^3.0.1",
    "arrayslicer": "^1.2.3",
    "hammerjs": "^2.0.8",
    "hamsterjs": "^1.1.3",
    "lz-string": "^1.5.0",
    "moment": "^2.29.4",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-svelte": "^3.11.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "vite": "^4.5.0",
    "vite-plugin-banner": "^0.7.1",
    "ws": "^8.14.2"
  }
}
